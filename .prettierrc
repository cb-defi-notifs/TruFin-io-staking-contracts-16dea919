{
    "semi": true,
    "trailingComma": "none",
    "printWidth": 120,
    "plugins": ["prettier-plugin-solidity"],
    "overrides": [
        {
          "files": "*.sol",
          "options": {
            "printWidth": 120,
            "tabWidth": 4,
            "useTabs": false,
            "singleQuote": false,
            "bracketSpacing": false
          }
        }
      ]
}
